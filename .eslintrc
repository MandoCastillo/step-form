{
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": 2,
    "react/react-in-jsx-scope": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".ts", ".tsx"] }]
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["airbnb", "airbnb/hooks", "plugin:prettier/recommended"]
}